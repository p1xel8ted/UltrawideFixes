<Project>
    <PropertyGroup>
        <!-- Name of your game folder -->
        <GameFolderName>Broforce</GameFolderName>
        
         <!-- Path to your game installation -->
        <GamePath>G:\SteamLibrary\steamapps\common\Broforce</GamePath>
        
        <!-- Path to your game's managed folder -->
        <ManagedPath>G:\SteamLibrary\steamapps\common\Broforce\Broforce_beta_Data\Managed</ManagedPath>
  
        <!-- Path to your BepInEx plugins folder -->
        <BepInExPluginsPath>G:\SteamLibrary\steamapps\common\Broforce\BepInEx\plugins</BepInExPluginsPath>

        <!-- Path to your stripped libs folder -->
        <StrippedLibs>$(MSBuildProjectDirectory)\libs</StrippedLibs>
        
        <ConfigManagerPath>..\..\ConfigManager</ConfigManagerPath>
    </PropertyGroup>
</Project>