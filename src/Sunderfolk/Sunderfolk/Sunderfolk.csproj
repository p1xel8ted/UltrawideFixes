<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Product>Sunderfolk Ultra-Wide</Product>
        <Version>0.1.0</Version>
        <AssemblyName>SunderfolkUltraWide</AssemblyName>
        <AssemblyVersion>$(Version)</AssemblyVersion>
        <FileVersion>$(Version)</FileVersion>
		        <RestoreAdditionalProjectSources>
            https://api.nuget.org/v3/index.json;
            https://nuget.bepinex.dev/v3/index.json;
            https://nuget.samboy.dev/v3/index.json
        </RestoreAdditionalProjectSources>
    </PropertyGroup>

    <Import Project="../../zip.target"/>
  
    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
      <DefineConstants>TRACE;BepInEx_IL2CPP</DefineConstants>
      <OutputPath>E:\SteamLibrary\steamapps\common\Sunderfolk\BepInEx\plugins</OutputPath>
      <DebugType>embedded</DebugType>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <DefineConstants>TRACE;BepInEx_IL2CPP</DefineConstants>
      <OutputPath>E:\SteamLibrary\steamapps\common\Sunderfolk\BepInEx\plugins</OutputPath>
      <DebugSymbols>true</DebugSymbols>
      <DebugType>embedded</DebugType>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="BepInEx.Unity.IL2CPP" Version="6.0.0-be.*" IncludeAssets="compile"/>
    </ItemGroup>
    
